{
	"format_version": "1.20.80",
	"minecraft:entity": {
		"description": {
			"identifier": "poke:zombken",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:parrot"
		},
		"component_groups": {
			"despawn":{
				"minecraft:instant_despawn": {
					"remove_child_entities": false
				}
			},
			"timer":{
				"minecraft:timer": {
					"time": 1800,
					"looping": true,
					"time_down_event": {
						"event": "poke:despawn",
						"target": "self"
					}
				}
			}
		},
		"components": {
			"minecraft:game_event_movement_tracking": {
				"emit_flap": true
			},
			"minecraft:nameable": {
				"name_actions":{
					"name_filter": "despawn",
					"on_named": {
						"event": "poke:despawn",
						"target": "self"
					}
				},
				"default_trigger": {
					"target": "self",
					"event": "poke:renamed"
				}
			},
			"minecraft:on_death": {
				"event": "death_sound"
			},
			"minecraft:type_family": {
				"family": [
					"monster",
					"mob"
				]
			},
			"minecraft:on_hurt_by_player": {
				"event": "hurt_sound"
			},
			"minecraft:scale": {
				"value": 2
			},
			"minecraft:loot": {
				"table": "loot_tables/poke/entities/zombken.json"
			},
			"minecraft:experience_reward": {
				"on_bred": "math.random(10,70)",
				"on_death": "query.last_hit_by_player?5:0"
			},
			"minecraft:health": {
				"value": 200,
				"max": 200
			},
			"minecraft:knockback_resistance": {
				"value": 0.5
			},
			"minecraft:attack": {
				"damage": 8,
				"effect_name": "hunger",
				"effect_duration": 20
			},
			"minecraft:behavior.melee_box_attack": {
				"melee_fov": 90,
				"priority": 3,
				"speed_multiplier": 1,
				"track_target": true,
				"require_complete_path": false
			},
			"minecraft:behavior.hurt_by_target": {
				"priority": 1
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 2,
				"must_see": false,
				"scan_interval": 10,
				"within_radius": 250,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "player"
								}
							]
						},
						"must_see": false
					}
				]
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": 0,
				"breathes_water": true
			},
			"minecraft:movement": {
				"value": 0.25
			},
			"minecraft:navigation.walk": {
				"is_amphibious": false,
				"can_float": true,
				"can_path_over_water": true,
				"avoid_damage_blocks": true,
				"can_sink": false
			},
			"minecraft:movement.basic": {},
			"minecraft:jump.static": {},
			"minecraft:can_climb": {},
			"minecraft:behavior.random_stroll": {
				"priority": 7,
				"speed_multiplier": 1
			},
			"minecraft:behavior.random_look_around": {
				"priority": 9
			},
			"minecraft:behavior.look_at_player": {
				"priority": 8,
				"look_distance": 6,
				"probability": 0.02
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": true,
				"is_pushable_by_piston": true
			},
			"minecraft:conditional_bandwidth_optimization": {},
			"minecraft:boss": {
				"hud_range": 55,
				"name": "entity.poke:zombken.name",
				"should_darken_sky": true
			},
			"minecraft:behavior.move_towards_target": {
				"priority": 15,
				"within_radius": 0.9,
				"speed_multiplier": 2
			},
			"minecraft:angry": {
				"broadcast_anger": true
			},
			"minecraft:attack_damage": {
				"value": 14
			},
			"minecraft:despawn": {
				"despawn_from_inactivity": true,
				"despawn_from_distance": {"max_distance": 128},
				"min_range_inactivity_timer": 100
			},
			"minecraft:on_hurt": {
				"event": "hurt_sound",
				"target": "self"
			},
			"minecraft:collision_box": {
				"height": 0.8,
				"width": 0.4
			},
			"minecraft:custom_hit_test": {
				"hitboxes": [
					{
						"width": 0.8,
						"height": 2.3,
						"pivot": [0, 0.5, 0.1]
					}
				]
			}
		},
		"events": {
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"timer"
					]
				}
			},
			"death_sound": {
				"queue_command": {
					"command": [
						"playsound play.listenerboss.victory @a[r=25]"
					]
				}
			},
			"hurt_sound": {
				"queue_command": {
					"command": [
						"particle poke:zombken_feather ~ ~ ~"
					]
				}
			},
			"poke:despawn":{
				"add": {
					"component_groups": ["despawn"]
				}
			},
			"poke:renamed":{
				"remove": {
					"component_groups": [
						"timer"
					]
				}
			}
		}
	}
}