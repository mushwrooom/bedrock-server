{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.speed_boots": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"move": "query.ground_speed"
						}
					]
				},
				"move": {
					"animations": [
						"move"
					],
					"transitions": [
						{
							"idle": "!query.ground_speed"
						}
					],
					"blend_transition": 1.25
				}
			}
		}
	}
}